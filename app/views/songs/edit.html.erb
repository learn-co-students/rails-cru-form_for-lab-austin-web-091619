<h1> <%= @model.class %> Form </h1>
<%= form_for(@model) do |f| %>
    <%= f.label :name %><br>
    <%=f.text_field :name %><br>
    <%=f.label :artist_id %><br>
    <%=f.text_field :artist_id %><br>
    <%=f.label :genre_id %><br>
    <%=f.text_field :genre_id %><br>
    <%=f.submit %>
    <%end %>
    